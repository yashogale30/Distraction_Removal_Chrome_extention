{
  "manifest_version": 3,
  "name": "Manual Distraction Remover",
  "version": "1.0",
  "description": "Manually remove distracting elements from any website.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img.png",
      "48": "img.png",
      "128": "img.png"
    }
  }
}
